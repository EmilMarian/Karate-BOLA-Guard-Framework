            "title": "Person",
            "type": "object",
            "x-expandableFields": [
              "additional_tos_acceptances",
              "address",
              "address_kana",
              "address_kanji",
              "dob",
              "future_requirements",
              "registered_address",
              "relationship",
              "requirements",
              "verification"
