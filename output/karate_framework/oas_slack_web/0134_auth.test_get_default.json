{
  "method": "GET",
  "parameters": [
    {
      "description": "Authentication token. Requires scope: `none`",
      "in": "header",
      "name": "token",
      "required": true,
      "schema": {
        "type": "string"
      }
    }
  ],
  "path": "/auth.test",
  "requestBody": {},
  "responses": {
    "default": {
      "content": {
        "application/json": {
          "examples": {
            "response": {
              "value": {
                "error": "invalid_auth",
                "ok": false
              }
            }
          },
          "schema": {
            "additionalProperties": false,
            "description": "Schema for error response auth.test method",
            "properties": {
              "callstack": {
                "description": "Note: PHP callstack is only visible in dev/qa",
                "type": "string"
              },
              "error": {
                "enum": [
                  "not_authed",
                  "invalid_auth",
                  "token_revoked",
                  "account_inactive",
                  "invalid_arg_name",
                  "invalid_array_arg",
                  "invalid_charset",
                  "invalid_form_data",
                  "invalid_post_type",
                  "missing_post_type",
                  "invalid_json",
                  "json_not_object",
                  "request_timeout",
                  "upgrade_required"
                ],
                "type": "string"
              },
              "ok": {
                "enum": [
                  false
                ],
                "title": "default failure response",
                "type": "boolean"
              }
            },
            "required": [
              "ok",
              "error"
            ],
            "title": "auth.test error schema",
            "type": "object"
          }
        }
      },
      "description": "Standard failure response when used with an invalid token"
    }
  }
}