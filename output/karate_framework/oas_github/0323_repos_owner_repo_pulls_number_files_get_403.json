      "schema": {
        "type": "string"
      }
    },
    {
      "description": "Name of repository.",
      "in": "path",
      "name": "repo",
      "required": true,
      "schema": {
        "type": "string"
      }
    },
    {
      "description": "Id of pull.",
      "in": "path",
      "name": "number",
